{#
/**
 * @file doctors.html.twig
 * Default theme implementation to present Doctors data.
 *
 * This template is used when viewing Doctors pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_doctors()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('doctors') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
